<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="digitalhouse.android.a0317moacns1c_02.Activities.MainActivity">

    <ImageView
        android:layout_width="300dp"
        android:layout_height="175dp"
        android:src="@drawable/logo"
        android:layout_centerHorizontal="true"
        android:id="@+id/imageViewLogoAM"
        android:layout_marginBottom="30dp"
        />
    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/animationViewMainActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:lottie_fileName="preloader.json"
        android:layout_below="@+id/imageViewLogoAM"
        app:lottie_scale="2"
        app:lottie_loop="true"
        app:lottie_autoPlay="true" />
</RelativeLayout>
