<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000"
    android:transitionName="imageviewer"
    tools:context="digitalhouse.android.a0317moacns1c_02.Activities.ImageViewerActivity">
    <android.support.v4.view.ViewPager
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/viewPagerImageView">
    </android.support.v4.view.ViewPager>
    <digitalhouse.android.a0317moacns1c_02.CustomViews.PageIndicator
        android:layout_width="wrap_content"
        android:layout_height="15dp"
        android:layout_gravity="center_horizontal|bottom"
        android:id="@+id/pageIndicatorImageView"/>
</FrameLayout>
